t;\n  };\n\n  const parseTime = (time) => {\n    const amPm = time > 1200 ? \"PM\" : \"AM\";\n    const temp = time.slice(0, 2) + \":\" + time.slice(2) + amPm;\n    return temp;\n  };\n\n  return done ? (\n    <div>\n      <Modal open={open}>\n        <div id=\"modalRoot\">\n          <div id=\"firstHalf\">\n            <div id=\"title\">{title}</div>\n\n            <div id=\"dateTime\">\n              <p> {parseDate(date)}</p>\n              <p> {parseTime(time)}</p>\n            </div>\n            <div id=\"seatPrice\">\n              <p>{seat.join(\", \")}</p>\n              <p>198,000원</p>\n            </div>\n          </div>\n          <hr className=\"solid\" />\n          <div id=\"bottomHalf\">\n            <p className=\"subtitle\">예약이 완료되었습니다.</p>\n            <p className=\"subtitle\">예약번호 &nbsp;&nbsp;&nbsp;&nbsp; {id}</p>\n          </div>\n\n          <div\n            style={{\n              width: \"85%\",\n              marginTop: \"20vh\",\n              marginLeft: \"7.5%\",\n              marginBottom: \"1.5vh\",\n              textAlign: \"center\",\n              fontSize: \"min(1vw, 1.6vh)\",\n            }}\n          >\n            예약 조회는 ‘예약 번호로 조회' 또는 ‘생년월일과 휴대폰 번호로 조회'\n            모두 가능합니다\n          </div>\n          <div id=\"payButton\">\n            <Button\n              style={{\n                backgroundColor: \"lightGray\",\n                fontSize: \"min(2.8vh, 1.7vw)\",\n                color: \"white\",\n                width: \"85%\",\n              }}\n            >\n              <Link to=\"/\" style={{ textDecoration: \"none\" }}>\n                돌아가기\n              </Link>\n            </Button>\n          </div>\n        </div>\n      </Modal>\n    </div>\n  ) : (\n    <div>\n      <Modal open={open}>\n        <div id=\"modalRoot\">\n          <div id=\"firstHalf\">\n            <div id=\"title\">{title}</div>\n\n            <div id=\"dateTime\">\n              <p> {parseDate(date)}</p>\n              <p> {parseTime(time)}</p>\n            </div>\n            <div id=\"seatPrice\">\n              <p>{seat.join(\", \")}</p>\n              <p>198,000원</p>\n            </div>\n          </div>\n          <hr className=\"solid\" />\n\n          <div id=\"bottomHalf\">\n            <div id=\"enterInfo\">\n              <div className=\"textfield\">\n                <p className=\"subtitle\">생년월일(6자리)</p>\n                <TextField\n                  id=\"birthday\"\n                  value={birthday}\n                  onChange={handleChange}\n                  inputProps={{ maxLength: 11, style: { marginLeft: \"1vw\" } }}\n                  InputProps={{\n                    style: { fontSize: \"min(1.4vw, 2.3vh)\" },\n                  }}\n                  variant=\"standard\"\n                  style={{ width: \"60%\" }}\n                />\n              </div>\n              <div className=\"textfield\">\n                <p className=\"subtitle\">휴대폰 번호</p>\n                <TextField\n                  id=\"phone\"\n                  value={phone}\n                  onChange={handleChange}\n                  inputProps={{ maxLength: 11, style: { marginLeft: \"1vw\" } }}\n                  InputProps={{\n                    style: { fontSize: \"min(1.4vw, 2.3vh)\" },\n                  }}\n                  variant=\"standard\"\n                  style={{ width: \"60%\" }}\n                />\n              </div>\n              <div className=\"textfield\">\n                <p className=\"subtitle\">비밀번호(4자리)</p>\n                <TextField\n                  id=\"pwd\"\n                  type=\"password\"\n                  value={pwd}\n                  onChange={handleChange}\n                  inputProps={{ maxLength: 11, style: { marginLeft: \"1vw\" } }}\n                  InputProps={{\n                    style: { fontSize: \"min(1.4vw, 2.3vh)\" },\n                  }}\n                  variant=\"standard\"\n                  style={{ width: \"60%\" }}\n             